{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Content": {
      "type": "richtext",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "Published": {
      "type": "date"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "post"
    },
    "Comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "post"
    }
  }
}
